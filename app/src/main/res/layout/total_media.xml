<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
    <LinearLayout android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:orientation="vertical">
        <include
            layout="@layout/title"/>
        <LinearLayout android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginTop="5dp"
                      android:orientation="horizontal">
            <LinearLayout android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:layout_weight="1"
                          android:orientation="vertical">
                <FrameLayout android:layout_width="match_parent"
                             android:layout_height="130dp">
                    <ImageView
                        android:id="@+id/videoview1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/coach_pic"/>
                    <TextView android:id="@+id/time1"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_gravity="right|bottom"
                              android:background="@color/gray"
                              android:text="00:00:00"/>
                </FrameLayout>
                <TextView android:id="@+id/textView1"
                          android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:gravity="center"
                          android:paddingTop="10dp"
                          android:text="倒车入库"/>
            </LinearLayout>
            <LinearLayout android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:layout_marginLeft="5dp"
                          android:layout_weight="1"
                          android:orientation="vertical">
                <FrameLayout android:layout_width="match_parent"
                             android:layout_height="130dp">
                    <ImageView
                        android:id="@+id/videoview2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/coach_pic"/>
                    <TextView android:id="@+id/time2"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_gravity="right|bottom"
                              android:background="@color/gray"
                              android:text="00:00:00"/>
                </FrameLayout>
                <TextView android:id="@+id/textView2"
                          android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:gravity="center"
                          android:paddingTop="10dp"
                          android:text="倒车入库"/>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginTop="10dp"
                      android:orientation="horizontal">
            <LinearLayout android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:layout_weight="1"
                          android:orientation="vertical">
                <FrameLayout android:layout_width="match_parent"
                             android:layout_height="130dp">
                    <ImageView
                        android:id="@+id/videoview3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/coach_pic"/>
                    <TextView android:id="@+id/time3"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_gravity="right|bottom"
                              android:background="@color/gray"
                              android:text="00:00:00"/>
                </FrameLayout>
                <TextView android:id="@+id/textView3"
                          android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:gravity="center"
                          android:paddingTop="10dp"
                          android:text="倒车入库"/>
            </LinearLayout>
            <LinearLayout android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:layout_marginLeft="5dp"
                          android:layout_weight="1"
                          android:orientation="vertical">
                <FrameLayout android:layout_width="match_parent"
                             android:layout_height="130dp">
                    <ImageView
                        android:id="@+id/videoview4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/coach_pic"/>
                    <TextView android:id="@+id/time4"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_gravity="right|bottom"
                              android:background="@color/gray"
                              android:text="00:00:00"/>
                </FrameLayout>
                <TextView android:id="@+id/textView4"
                          android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:gravity="center"
                          android:paddingTop="10dp"
                          android:text="倒车入库"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginTop="10dp"
                      android:orientation="horizontal">
            <LinearLayout android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:layout_weight="1"
                          android:orientation="vertical">
                <FrameLayout android:layout_width="match_parent"
                             android:layout_height="130dp">
                    <ImageView
                        android:id="@+id/videoview5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/coach_pic"/>
                    <TextView android:id="@+id/time5"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_gravity="right|bottom"
                              android:background="@color/gray"
                              android:text="00:00:00"/>
                </FrameLayout>
                <TextView android:id="@+id/textView5"
                          android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:gravity="center"
                          android:paddingTop="10dp"
                          android:text="倒车入库"/>
            </LinearLayout>
            <LinearLayout android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:layout_marginLeft="5dp"
                          android:layout_weight="1"
                          android:visibility="invisible"
                          android:orientation="vertical">
                <FrameLayout android:layout_width="match_parent"
                             android:layout_height="130dp">
                    <ImageView
                        android:id="@+id/videoview6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/coach_pic"/>
                    <TextView android:id="@+id/time6"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_gravity="right|bottom"
                              android:background="@color/gray"
                              android:text="00:00:00"/>
                </FrameLayout>
                <TextView android:id="@+id/textView6"
                          android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:gravity="center"
                          android:paddingTop="10dp"
                          android:text="倒车入库"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</ScrollView>
